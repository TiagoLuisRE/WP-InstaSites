<script id="tmpl-propertysearch-galleryview" type="text/html">
<header class="row-fluid search-result-controls">
<div class="span7 form-horizontal">&nbsp;</div>
<div class="span5 form-horizontal">
<div class="control-group pull-right">
	<label class="control-label">View as</label>
	<div class="controls">
		<div class="btn-group" data-toggle="buttons-radio">
		<button class="btn changeview" data-template="tmpl-propertysearch-listview"><i class="icon-list"></i>&nbsp;{{textdata.List}}</button>
		<button class="btn changeview active" data-template="tmpl-propertysearch-galleryview"><i class="icon-th-large"></i>&nbsp;{{textdata.Photo}}</button>		
		</div>
        <button class="btn changeview" data-template="tmpl-propertysearch-galleryview" style="display:none"><i class="icon-map-marker"></i>&nbsp;{{textdata.Map}}</button>
	</div>
</div>
</div>	
</header>
<div class="gallery-view-page">
{{#result}}
<div class="span6">
<div class="portal-result">
    <div class="portal-inner">    
        <div class="portal-images">
			<div id="slider" class="flexslider bapi-flexslider" data-options='{ "animation": "slide", "controlNav": false, "animationLoop": false, "slideshow": false, "sync": "#carousel" }'>
			  <ul class="slides">
				  {{#Images}}
				  <li><a class="property-link" href="{{&ContextData.SEO.DetailURL}}"><img alt="{{Caption}}" src="{{&MediumURL}}" /></a></li>
				  {{/Images}} 
			  </ul>
			</div>           
		</div>       
		<div class="portal-info">
			<h2 class="property-title"><a href="{{&ContextData.SEO.DetailURL}}">{{Headline}}</a></h2>
			<div class="location"><span><b>{{textdata.City}}:</b> {{City}}</span> {{#Bedrooms}}<b>{{textdata.Beds}}</b>: {{Bedrooms}} | {{/Bedrooms}}{{#Bathrooms}}<b>{{textdata.Baths}}</b>: {{Bathrooms}} | {{/Bathrooms}}<b>{{textdata.Sleeps}}</b>: {{Sleeps}}</div>			
			<div class="portal-rates">
				{{^ContextData.Quote.IsValid}}
				<div class="system-msg-error">{{ContextData.Quote.ValidationMessage}}</div>
				{{/ContextData.Quote.IsValid}}
				{{#ContextData.Quote.IsValid}}
				{{ContextData.Quote.PublicNotes}}
				{{/ContextData.Quote.IsValid}}
			</div>			
		</div>
	</div>
</div>
</div>
{{/result}}
</div>
<div class="clearfix"></div>
{{^islastpage}}<div class="showmore"><a href='javascript:void(0)'><span class="span12 alert alert-info">{{textdata.Show More Results}} ({{totalcount}})</a></span></div>{{/islastpage}}
{{#islastpage}}<div class="nomore"><a href='javascript:void(0)'><span class="span12 alert alert-warn">{{textdata.There are no more results}}</a></span></div>{{/islastpage}}
</script>

